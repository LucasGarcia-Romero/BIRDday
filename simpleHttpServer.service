[Unit]
Description=Audiomoth USB Recording Service

[Service]
User=root
WorkingDirectory=/home/orangepi
ExecStart=/home/orangepi/FileServerBin/FileServer -p 80 -a 192.168.2.56 -r /home/orangepi/sdBackup/
Restart=always
RestartSec=3
StandardOutput=null
StandardError=null
[Install]
WantedBy=multi-user.target
